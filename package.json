{
  "name": "random-tale-of-kieu",
  "description": "Generate random from The Tale of Kieu - Nguyen Du",
  "version": "1.0.0",
  "repository": "git@github.com:huuquyet/random-tale-of-kieu",
  "author": "huuquyet <huuquyetng@gmail.com>",
  "bugs": {
    "url": "https://github.com/huuquyet/random-tale-of-kieu/issues"
  },
  "license": "MIT",
  "main": "./src/index.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "bundle": "yarn lint && yarn package",
    "clean": "git clean -xdf",
    "doctor": "yarn dlx @yarnpkg/doctor",
    "lint": "yarn dlx @biomejs/biome check --apply .",
    "package": "yarn ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "package:watch": "yarn package -- --watch",
    "update": "yarn upgrade-interactive",
    "vscode": "yarn dlx @yarnpkg/sdks vscode"
  },
  "dependencies": {
    "@actions/core": "^1.10.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@vercel/ncc": "^0.38.1",
    "typescript": "^5.4.5"
  },
  "packageManager": "yarn@4.2.2"
}
